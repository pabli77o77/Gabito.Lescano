@model AdSanare.Entities.Persona
<div class="card card-primary">
    <div class="card-header">
        <h3 class="card-title">Detalle de Paciente</h3>
    </div>
        <div class="card-body">
            <div class="form-group">
                <label asp-for="Nombre"></label>
                <input disabled class="form-control" asp-for="Nombre">
                <span class="text-danger" asp-validation-for="Nombre"></span>
            </div>
            <div class="form-group">
                <label asp-for="Apellido"></label>
                <input disabled class="form-control" asp-for="Apellido">
                <span class="text-danger" asp-validation-for="Apellido"></span>
            </div>
            <div class="form-group">
                <label asp-for="Documento"></label>
                <input disabled class="form-control" asp-for="Documento">
                <span class="text-danger" asp-validation-for="Documento"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaNacimiento"></label>
                <input disabled class="form-control" asp-for="FechaNacimiento">
                <span class="text-danger" asp-validation-for="FechaNacimiento"></span>
            </div>
            <div class="form-group">
                <label asp-for="ObraSocial"></label>
                <input disabled class="form-control" asp-for="ObraSocial">
                <span class="text-danger" asp-validation-for="ObraSocial"></span>
            </div>
            <div class="form-group">
                <label asp-for="ObraSocialNumero"></label>
                <input disabled class="form-control" asp-for="ObraSocialNumero">
                <span class="text-danger" asp-validation-for="ObraSocialNumero"></span>
            </div>
            <div class="form-group">
                <label asp-for="Diagnostico"></label>
                <input disabled class="form-control" asp-for="Diagnostico">
                <span class="text-danger" asp-validation-for="Diagnostico"></span>
            </div>
        </div>
        <div class="card-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button>
        </div>
</div>
