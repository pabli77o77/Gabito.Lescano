@model AdSanare.Entities.Ingreso
    </br>
    <div class="card card-maroon">
        <div class="card-header">
            <h3 class="card-title">Detalle del Ingreso</h3>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Paciente.Nombre"></label>
                        <input disabled class="form-control" asp-for="Paciente.Nombre">
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Paciente.Apellido"></label>
                        <input disabled class="form-control" asp-for="Paciente.Apellido">
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Paciente.Documento"></label>
                        <input disabled class="form-control" asp-for="Paciente.Documento">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="FechaIngreso"></label>
                        <input disabled class="form-control" asp-for="FechaIngreso">
                    </div>
                    <div class="form-group">
                        <label asp-for="Peso"></label>
                        <input disabled class="form-control" asp-for="Peso">
                    </div>
                    <div class="form-group">
                        <label asp-for="Talla"></label>
                        <input disabled class="form-control" asp-for="Talla">
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Diagnostico"></label>
                        <textarea disabled class="form-control" asp-for="Diagnostico" rows="8"></textarea>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="AntecedentesMedicos"></label>
                        <textarea disabled class="form-control" asp-for="AntecedentesMedicos" rows="8"></textarea>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="AntecedentesQuirurgicos"></label>
                        <textarea disabled class="form-control" asp-for="AntecedentesQuirurgicos" rows="6"></textarea>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Alergias"></label>
                        <textarea disabled class="form-control" asp-for="Alergias" rows="6"></textarea>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="MedicacionHabitual"></label>
                        <textarea disabled class="form-control" asp-for="MedicacionHabitual" rows="6"></textarea>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-footer text-center">
            <button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button>
        </div>
    </div>
